Interval 1

LoadPlugin "logfile"
<Plugin "logfile">
  LogLevel "debug"
  File stdout
  Timestamp true
</Plugin>

LoadPlugin cpu
LoadPlugin amqp1

<Plugin "amqp1">
  <Transport "name">
    Host "qdr-white.sa-telemetry.svc.cluster.local"
    Port "5672"
    Address "collectd"
    <Instance "<<CLOUDNAME>>-telemetry">
        Format JSON
        PreSettle true
    </Instance>
  </Transport>
</Plugin>
